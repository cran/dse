#!/bin/sh

# assuming the DSE R libraries are in the pwd set
#R_LIBS=${R_LIBS:=$PWD/rlibs} export R_LIBS
echo R_LIBS set to $R_LIBS

rm -f .RData
d=`date`

R --nsize=1M --vsize=6M --no-save --quiet <<eofR
