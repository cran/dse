#!/bin/sh

#R_LIBS=${R_LIBS:=$PWD/rlibs} export R_LIBS

rm -f .RData
d=`date`

# vsize 12000000 is too small at B5
# vsize 20000000 is too small at B6

#       Feb 26, 1999.
# next works and all tests complete ok with R 0.63.3pre but it is slow and
#    needs to be run on a fast computer with lots of memory
R --vsize=35M --nsize=300K --no-save --quiet <<eofR
# gcinfo(1)
  require("dse2"); require("dse3")  #,  warn.conflicts=F)
  source("test.dsecurveB.s")

eofR

echo "========================================="
echo "R test.dsecurveB.s timing:"
echo "start" $d
echo "end  " `date`
echo `uname -a`
echo "========================================="

rm -f .RData
