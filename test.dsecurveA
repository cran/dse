#!/bin/sh

#R_LIBS=${R_LIBS:=$PWD/rlibs} export R_LIBS

rm -f .RData
d=`date`

# -v8 -n400000  kills speed (on 16M memory machine)
# -v6 -n350000  dies with memory exhausted at test 12
# -v6 -n400000 227455 cons cells free (56%)  5453k bytes of heap free (88%)
R --vsize=10M --nsize=300K --no-save --quiet <<eofR
# gcinfo(1)
  require("dse2"); require("dse3") #,  warn.conflicts=F)
  source("test.dsecurveA.s")

eofR

echo "========================================="
echo "R test.dsecurveA.s timing:"
echo "start" $d
echo "end  " `date`
echo `uname -a`
echo "========================================="

rm -f .RData
